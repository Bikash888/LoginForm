<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane fx:id="anchorpane" prefHeight="400.0" prefWidth="650.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="application.Main">
   <children>
      <Text layoutX="228.0" layoutY="39.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome In Login Page">
         <font>
            <Font name="Agency FB Bold" size="25.0" />
         </font>
         <fill>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="#282525" />
                  <Stop color="#da5050" offset="1.0" />
               </stops>
            </LinearGradient>
         </fill>
      </Text>
      <JFXTextField fx:id="userText" focusColor="#0533bc" labelFloat="true" layoutX="185.0" layoutY="104.0" prefHeight="59.0" prefWidth="312.0" promptText="User Name">
         <font>
            <Font size="23.0" />
         </font>
      </JFXTextField>
      <JFXPasswordField fx:id="password" labelFloat="true" layoutX="185.0" layoutY="216.0" prefHeight="60.0" prefWidth="312.0" promptText="Password">
         <font>
            <Font size="28.0" />
         </font>
      </JFXPasswordField>
      <JFXButton fx:id="loginbtn" buttonType="RAISED" layoutX="185.0" layoutY="299.0" onAction="#loginaction" prefHeight="43.0" prefWidth="113.0" text="Login">
         <textFill>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="#131e39" />
                  <Stop color="#5b2525" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill>
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="create" layoutX="318.0" layoutY="299.0" onAction="#createaccount" prefHeight="43.0" prefWidth="193.0" text="Create New account">
         <textFill>
            <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
               <stops>
                  <Stop color="#14432a" />
                  <Stop color="#d95050" offset="1.0" />
               </stops>
            </RadialGradient>
         </textFill>
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
